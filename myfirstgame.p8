__lua__
function _init()
	player = {x=64,y=64}
end

function _update()
	if btn(0) then player.x -= 1 end
	if btn(1) then player.x += 1 end
	if btn(2) then player.y -= 1 end
	if btn(3) then player.y += 1	end	

	if btn(4) then sfx(0) end
end

function _draw()
	cls()
	map(0,0,0,0,16,16)
	spr(1, player.x, player.y)
end


