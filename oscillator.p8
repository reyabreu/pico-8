function oscillate(from,to,duration,phase)
	phase=phase==nil and 0 or phase
	local pi=3.1415
	local tau=pi*2
	local dis=(to-from)/2
	duration/=120
	return from+dis+sin((t()/100+to*tau)/duration-phase/360)*dis
end
